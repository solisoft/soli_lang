    <!-- Array Functions -->
    <section id="section-arrays" class="scroll-mt-20 mb-12">
        <h2 class="text-2xl font-bold text-white mb-4">Array Functions</h2>

        <div class="bg-yellow-500/10 border border-yellow-500/20 rounded-xl p-4 mb-6">
            <p class="text-yellow-300 text-sm">
                <strong>Tip:</strong> Most array functions are also available as <a href="/docs/language/arrays" class="underline hover:text-yellow-200">Array class methods</a>.
                Use <code class="text-yellow-300">arr.push(42)</code> instead of <code class="text-yellow-300">push(arr, 42)</code> for a more object-oriented style.
            </p>
        </div>

        <div class="space-y-6">
            <section id="fn-push" class="scroll-mt-20">
                <div class="rounded-xl bg-white/5 border border-white/10 p-5">
                    <a href="#fn-push" class="group flex items-center gap-2 mb-2">
                        <code class="text-lg font-mono text-indigo-400">push(array, value)</code> / <code class="text-lg font-mono text-indigo-400">array.push(value)</code>
                        <svg class="w-4 h-4 opacity-0 group-hover:opacity-100 text-gray-500 transition-opacity" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13.828 10.172a4 4 0 00-5.656 0l-4 4a4 4 0 105.656 5.656l1.102-1.101m-.758-4.899a4 4 0 005.656 0l4-4a4 4 0 00-5.656-5.656l-1.1 1.1" />
